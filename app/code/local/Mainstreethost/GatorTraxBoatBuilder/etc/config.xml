<?xml version="1.0"?>
<config>
    <modules>
        <Mainstreethost_GatorTraxBoatBuilder>
            <version>0.1.0</version>
        </Mainstreethost_GatorTraxBoatBuilder>
    </modules>
    <frontend>
        <routers>
            <boatbuilder>
                <use>standard</use>
                <args>
                    <module>Mainstreethost_GatorTraxBoatBuilder</module>
                    <frontName>boatbuilder</frontName>
                </args>
            </boatbuilder>
        </routers>
    </frontend>
    <global>
        <models>
            <gator>
                <class>Mainstreethost_GatorTraxBoatBuilder_Model</class>
                <resourceModel>gator_resource</resourceModel>
            </gator>
            <gator_resource>
                <class>Mainstreethost_GatorTraxBoatBuilder_Model_Resource</class>
            </gator_resource>
        </models>
        <blocks>
            <gator>
                <class>Mainstreethost_GatorTraxBoatBuilder_Block</class>
            </gator>
        </blocks>
        <helpers>
            <gator>
                <class>Mainstreethost_GatorTraxBoatBuilder_Helper</class>
            </gator>
        </helpers>
        <events>
            <controller_action_predispatch>
                <observers>
                    <Mainstreethost_gatortraxboatbuilder_model_observer>
                        <type>model</type>
                        <class>Mainstreethost_GatorTraxBoatBuilder_Model_Observer</class>
                        <method>getGatorProducts</method>
                    </Mainstreethost_gatortraxboatbuilder_model_observer>
                </observers>
            </controller_action_predispatch>
        </events>
    </global>
    <adminhtml>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <boatbuildersettings>
                                            <title>Boat Builder Settings</title>
                                        </boatbuildersettings>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
</config>